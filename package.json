    "scripts": {
        "test-webpack-5": "cross-env WEBPACK_PACKAGE=webpack jest",
        "test-webpack-4": "cross-env WEBPACK_PACKAGE=webpack-v4 jest",
        "test-code-style": "echo 'If thereâ€™re any files listed below, this means they break the code style. Fix them with: yarn fix-code-style' && prettier --list-different '**/*.js'",
